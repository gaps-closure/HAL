node n1 {
    type router
    model host
    network-config {
	hostname n1
	!
	interface eth1
	 ip address 10.3.1.1/24
	!
	interface eth0
	 ip address 10.2.1.1/24
	!
    }
    canvas c1
    iconcoords {138.0 201.0}
    labelcoords {138.0 233.0}
    interface-peer {eth0 n3}
    interface-peer {eth1 n4}
}

node n2 {
    type router
    model host
    network-config {
	hostname n2
	!
	interface eth1
	 ip address 10.3.2.1/24
	!
	interface eth0
	 ip address 10.2.2.1/24
	!
    }
    canvas c1
    iconcoords {513.0 196.0}
    labelcoords {513.0 228.0}
    interface-peer {eth0 n3}
    interface-peer {eth1 n4}
}

node n3 {
    type hub
    network-config {
	hostname n3
	!
    }
    canvas c1
    iconcoords {320.0 134.0}
    labelcoords {320.0 158.0}
    interface-peer {e0 n1}
    interface-peer {e1 n2}
}

node n4 {
    type hub
    network-config {
	hostname n4
	!
    }
    canvas c1
    iconcoords {323.0 242.0}
    labelcoords {323.0 266.0}
    interface-peer {e0 n1}
    interface-peer {e1 n2}
}

link l1 {
    nodes {n1 n3}
    bandwidth 0
}

link l2 {
    nodes {n3 n2}
    bandwidth 0
}

link l3 {
    nodes {n1 n4}
    bandwidth 0
}

link l4 {
    nodes {n4 n2}
    bandwidth 0
}

canvas c1 {
    name {Canvas1}
}

option global {
    interface_names no
    ip_addresses yes
    ipv6_addresses yes
    node_labels yes
    link_labels yes
    show_api no
    background_images no
    annotations yes
    grid yes
    traffic_start 0
}

option session {
}

